<html lang="en">
    <head>
        <title>ConUshop | Home </title>
        <% include ../components/header.ejs %>
    </head>
    <body>
    <script type="text/javascript" src="/js/bootstrap-3.1.1.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/list.js/1.5.0/list.min.js"></script>
    <script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.4.4/underscore-min.js"></script>
    <script src='https://cdnjs.cloudflare.com/ajax/libs/multiple-select/1.2.0/multiple-select.js'></script>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/multiple-select/1.2.0/multiple-select.min.css" rel="stylesheet" type="text/css" media="all" />
    <script type="text/javascript" src="/js/main.js"></script>
    
    <% include ../components/navigator.ejs %>
    <% include ../components/clientNav.ejs %>
    
    <div>
        <% if (typeof data!='undefined') { %>
            <table>
                <tr>
                    <th>Serial #</th>
                    <th>Model</th>
                    <th>Brand</a></th>
                    <th>Processor</th>
                    <th>RAM</a></th>
                    <th>Storage</a></th>
                    <th>Cores</a></th>
                    <th>Dimensions</th>
                    <th>Weight</a></th>
                    <th>Price</a></th>
                </tr>
                <% for(var i = 0; i < data.length; i++) { %>
                    <tr>
                        <td class="serial"><%= data[i].itemObj.serialNumber %></td>
                        <td class="model"><%= data[i].itemObj.modelNumber %></td>
                        <td class="brand"><%= data[i].specification.brand %></td>
                        <td class="processor"><%= data[i].specification.processor %></td>
                        <td class="ram"><%= data[i].specification.ram %></td>
                        <td class="storage"><%= data[i].specification.storage %></td>
                        <td class="cores"><%= data[i].specification.cores %></td>
                        <td class="dimensions"><%= data[i].specification.dimensions %></td>
                        <td class="weight"><%= data[i].specification.weight %></td>
                        <td class="price"><%= data[i].specification.price %></td>
                        <td><button onclick="deleteCartItem('<%= data[i].serialNumber %>')">Delete</button></td>
                    </tr>
                <% } %>
            </table>
        <% } %>
        <td><button onclick="makePurchase()">Make Purchase</button></td>
    </div>
    
    <!-- <script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.4.4/underscore-min.js"></script> -->
    <!-- <script src='https://cdnjs.cloudflare.com/ajax/libs/multiple-select/1.2.0/multiple-select.js'></script> -->
    <script type="text/javascript" src="/js/catalog/catalog.js"></script>
    <% include ../components/footer.ejs %>
    </body>
    </html>
    